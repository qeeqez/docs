{
  "name": "rixl-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "bun lint && next build",
    "dev": "next dev",
    "serve": "bunx serve@latest out",
    "lint": "oxlint . && fumadocs-mdx && bun ./scripts/lint.ts",
    "lint:fix": "oxlint --fix .",
    "format": "oxfmt",
    "postinstall": "fumadocs-mdx",
    "prepare": "husky"
  },
  "dependencies": {
    "@orama/orama": "^3.1.18",
    "@radix-ui/react-presence": "^1.1.5",
    "@rixl/videosdk-react": "^0.18.2",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "16.4.6",
    "fumadocs-mdx": "14.2.4",
    "fumadocs-openapi": "^10.2.4",
    "fumadocs-ui": "16.4.6",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "shiki": "^3.21.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bun": "^1.3.5",
    "@types/mdx": "^2.0.13",
    "@types/node": "25.0.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-validate-link": "^1.6.4",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "wrangler": "^4.58.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bunx oxlint --fix",
      "bunx oxfmt"
    ]
  },
  "packageManager": "bun@1.3.3"
}
